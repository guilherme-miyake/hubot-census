).\n\n\n","maintainers":[{"name":"neufeldtech","email":"myjc.niv@gmail.com"}],"time":{"modified":"2017-03-15T04:13:09.790Z","created":"2016-10-18T03:10:19.711Z","0.0.1":"2016-10-18T03:10:19.711Z","0.0.2":"2017-03-15T04:13:09.790Z"},"homepage":"https://github.com/neufeldtech/hubot-orly#readme","keywords":["hubot","hubot-scripts","hubot-orly"],"repository":{"type":"git","url":"git+https://github.com/neufeldtech/hubot-orly.git"},"author":{"name":"Jordan Neufeld","email":"myjc.niv@gmail.com"},"bugs":{"url":"https://github.com/neufeldtech/hubot-orly/issues"},"license":"MIT","readmeFilename":"README.md"}
