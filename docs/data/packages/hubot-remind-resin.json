dalmut](https://github.com/wdalmut).\n","maintainers":[{"name":"sqweelygig","email":"andrewl@resin.io"}],"time":{"modified":"2017-02-12T17:26:57.496Z","created":"2017-02-12T17:26:57.496Z","0.1.5":"2017-02-12T17:26:57.496Z"},"homepage":"https://github.com/clippings/hubot-remind#readme","repository":{"type":"git","url":"git+https://github.com/clippings/hubot-remind.git"},"bugs":{"url":"https://github.com/clippings/hubot-remind/issues"},"readmeFilename":"README.md","_attachments":{}}