js.com/package/hubot-fifty-shades\n","maintainers":[{"name":"simonsmith","email":"me@simonsmith.io"}],"time":{"modified":"2016-09-16T19:13:41.562Z","created":"2016-09-15T14:54:21.693Z","0.1.0":"2016-09-15T14:54:21.693Z","0.2.0":"2016-09-16T19:13:41.562Z"},"homepage":"https://github.com/simonsmith/hubot-hubot-fifty-shades#readme","keywords":["fifty","shades"],"repository":{"type":"git","url":"git://github.com/simonsmith/hubot-hubot-fifty-shades.git"},"author":{"name":"Simon Smith","email":"simonsmith@users.noreply.github.com"},"bugs":{"url":"https://github.com/hubot-scripts/hubot-hubot-fifty-shades/issues"},"license":"MIT","readmeFilename":"README.md","_attachments":{}}