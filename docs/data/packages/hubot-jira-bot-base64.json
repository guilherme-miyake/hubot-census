\\"1\\\"\\},\\{\\\"name\\\":\\\"Critical\\\",\\\"id\\\":\\\"2\\\"\\},\\{\\\"name\\\":\\\"Major\\\",\\\"id\\\":\\\"3\\\"\\},\\{\\\"name\\\":\\\"Minor\\\",\\\"id\\\":\\\"4\\\"\\},\\{\\\"name\\\":\\\"Trivial\\\",\\\"id\\\":\\\"5\\\"\\}\\]`\n- `HUBOT_GITHUB_TOKEN` - Github Application Token\n","maintainers":[{"name":"jinglemansweep","email":"jinglemansweep@gmail.com"}],"time":{"modified":"2016-03-18T10:59:04.137Z","created":"2016-03-18T10:19:07.276Z","5.0.0":"2016-03-18T10:19:07.276Z","5.0.1":"2016-03-18T10:35:35.434Z","5.0.2":"2016-03-18T10:59:04.137Z"},"homepage":"https://github.com/jinglemansweep/hubot-jira-bot","keywords":["hubot","hubot-scripts","slack","jira","task","bug"],"repository":{"type":"git","url":"git+ssh://git@github.com/jinglemansweep/hubot-jira-bot.git"},"author":{"name":"JingleManSweep","email":"jinglemansweep@gmail.com"},"bugs":{"url":"https://github.com/jinglemansweep/hubot-jira-bot/issues"},"license":"MIT","readmeFilename":"README.md","_attachments":{}}