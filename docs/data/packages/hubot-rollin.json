lin-1.0.3.tgz_1487040423317_0.8329771521966904"},"directories":{}},"1.1.0":{"name":"hubot-rollin","description":"A hubot script that allows for advanced dice rolls.","version":"1.1.0","author":{"name":"James Liu","email":"james@jamesliu.ca"},"license":"MIT","keywords":["hubot","hubot-scripts","dice","roll","rpg"],"repository":{"type":"git","url":"git://github.com/yeah568/hubot-rollin.git"},"bugs":{"url":"https://github.com/yeah568/hubot-rollin/issues"},"dependencies":{},"peerDependencies":{"hubot":"2.x"},"devDependencies":{"hubot-test-helper":"^1.3.0","chai":"^2.1.1","coffee-script":"1.6.3","grunt":"^0.4.5","grunt-cli":"^0.1.13","grunt-contrib-watch":"~0.6.1","grunt-mocha-test":"~0.12.7","grunt-release":"~0.11.0","hubot":"2.x","matchdep":"~0.3.0","mocha":"^2.1.0","sinon":"^1.13.0","sinon-chai":"^2.7.0"},"main":"src/rollin.js","scripts":{"test":"grunt test"},"gitHead":"9d874ec5e516e11fa4d6f3c9cb2bc070f32108c4","homepage":"https://github.com/yeah568/hubot-rollin#readme","_id":"hubot-rollin@1.1.0","_shasum":"85fe37ff1b67b9aed65d044ff9e6161d1363ac61","_from":".","_npmVersion":"4.2.0","_nodeVersion":"6.6.0","_npmUser":{"name":"yeah568","email":"npm@jamesliu.ca"},"dist":{"shasum":"85fe37ff1b67b9aed65d044ff9e6161d1363ac61","tarball":"https://registry.npmjs.org/hubot-rollin/-/hubot-rollin-1.1.0.tgz"},"maintainers":[{"name":"yeah568","email":"npm@jamesliu.ca"}],"_npmOperationalInternal":{"host":"packages-18-east.internal.npmjs.com","tmp":"tmp/hubot-rollin-1.1.0.tgz_1487056429970_0.5558425295166671"},"directories":{}}},"readme":"# hubot-rollin\n\nA hubot script that allows for advanced dice rolls.\n\nSee [`src/rollin.js`](src/rollin.js) for full documentation.\n\n## Installation\n\nIn hubot project repo, run:\n\n`npm install hubot-rollin --save`\n\nThen add **hubot-rollin** to your `external-scripts.json`:\n\n```json\n[\n  \"hubot-rollin\"\n]\n```\n\n## Sample Interaction\n\n```\nuser>> hubot roll d20\nhubot>> user rolled:\n        16.\nuser>> hubot roll 3d20\nhubot>> user rolled:\n        16, 12, 4. Sum: 32.\nuser>> hubot roll 2:2d20\nhubot>> user rolled:\n        10, 4. Sum: 14.\n        3, 17. Sum: 20.\nuser>> hubot roll 2d20+3\nhubot>> user rolled:\n        8, 7. Sum: 15 + 3 = 18.\nuser>> hubot roll 2:3d20+4\nhubot>> user rolled:\n        13, 2, 12. Sum: 27 + 4 = 31.\n        17, 18, 4. Sum: 39 + 4 = 43.\n```\n\n## NPM Module\n\nhttps://www.npmjs.com/package/hubot-rollin\n","maintainers":[{"name":"yeah568","email":"npm@jamesliu.ca"}],"time":{"modified":"2017-02-14T07:13:51.718Z","created":"2017-02-12T05:28:24.530Z","1.0.0":"2017-02-12T05:28:24.530Z","1.0.1":"2017-02-12T05:52:40.640Z","1.0.2":"2017-02-13T05:12:20.122Z","1.0.3":"2017-02-14T02:47:05.207Z","1.1.0":"2017-02-14T07:13:51.718Z"},"homepage":"https://github.com/yeah568/hubot-rollin#readme","keywords":["hubot","hubot-scripts","dice","roll","rpg"],"repository":{"type":"git","url":"git://github.com/yeah568/hubot-rollin.git"},"author":{"name":"James Liu","email":"james@jamesliu.ca"},"bugs":{"url":"https://github.com/yeah568/hubot-rollin/issues"},"license":"MIT","readmeFilename":"README.md","users":{"yeah568":true},"_attachments":{}}